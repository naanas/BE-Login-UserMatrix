{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/login",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/register",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/profile",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/manage/users",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/users",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/manage/users/:id",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}